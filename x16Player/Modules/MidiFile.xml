		<hex>26</hex>

		<cpu:RollImmediate8AddressLeft/>
		<hex>27</hex>

		<cpu:ShiftImmediate8AddressLeft/>
		<hex>25</hex>

		<cpu:RollImmediate8AddressLeft/>
		<hex>26</hex>

		<cpu:RollImmediate8AddressLeft/>
		<hex>27</hex>

		<cpu:ReturnFromSubroutine/>
	</label>

	<label id="readQuantity">

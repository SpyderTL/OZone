	<label id="Position" export="ScreenRenderer.Position"/>
	<short>0</short>

	<label id="Color" export="ScreenRenderer.Color"/>
	<hex>00</hex>

	<label id="DrawString" export="ScreenRenderer.DrawString">
		<!--Get Length-->
		<cpu:CallImmediate16Address/>
		<addressOf ref="MemoryReader.ReadByte" type="Absolute16"/>

		<cpu:CopyAccumulatorToImmediate8Address/>
		<hex>02</hex>

		<cpu:CallImmediate16Address/>
		<addressOf ref="MemoryReader.ReadByte" type="Absolute16"/>

		<cpu:CopyAccumulatorToImmediate8Address/>
		<hex>03</hex>

		<!--Calculate Position-->
		<cpu:CopyImmediate16AddressToAccumulator/>
		<addressOf ref="Position" type="Absolute16"/>

		<cpu:ShiftAccumulatorLeft/>

		<cpu:CopyAccumulatorToImmediate8Address/>
		<hex>04</hex>

		<cpu:CopyImmediate16AddressToAccumulator/>
		<addressOf ref="Position" type="Absolute16" offset="1"/>

		<cpu:RollAccumulatorLeft/>

		<cpu:CopyAccumulatorToImmediate8Address/>
		<hex>05</hex>

		<cpu:CopyImmediate8ToAccumulator/>
		<hex>11</hex>

		<cpu:CopyAccumulatorToImmediate16Address/>
		<addressOf ref="Video.AddressHigh" type="Absolute16"/>

		<cpu:CopyImmediate8ToAccumulator/>
		<hex>b0</hex>

		<cpu:ClearCarryFlag/>

		<cpu:AddImmediate8AddressToAccumulator/>
		<hex>05</hex>

		<cpu:CopyAccumulatorToImmediate16Address/>
		<addressOf ref="Video.AddressMid" type="Absolute16"/>

		<cpu:CopyImmediate8ToAccumulator/>
		<hex>00</hex>

		<cpu:AddImmediate8AddressToAccumulator/>
		<hex>04</hex>

		<cpu:CopyAccumulatorToImmediate16Address/>
		<addressOf ref="Video.AddressLow" type="Absolute16"/>

		<label id="forEachCharacter"/>

		<cpu:CallImmediate16Address/>
		<addressOf ref="MemoryReader.ReadByte" type="Absolute16"/>

		<cpu:CopyAccumulatorToXIndex/>

		<cpu:CopyImmediate16PlusXIndexAddressToAccumulator/>
		<addressOf ref="screenCodes" type="Absolute16"/>

		<cpu:CopyAccumulatorToImmediate16Address/>
		<addressOf ref="Video.Data0" type="Absolute16"/>

		<cpu:CopyImmediate16AddressToAccumulator/>
		<addressOf ref="Color" type="Absolute16"/>

		<cpu:CopyAccumulatorToImmediate16Address/>
		<addressOf ref="Video.Data0" type="Absolute16"/>

		<cpu:DecrementImmediate8Address/>
		<hex>02</hex>

		<cpu:BranchToRelative8IfNotZero/>
		<addressOf ref="forEachCharacter" type="Relative8"/>

		<cpu:ReturnFromSubroutine/>
	</label>


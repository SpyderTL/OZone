			<hex>9f71</hex>

			<label id="done"/>

			<cpu:CopyImmediate16AddressToAccumulator/>
			<addressOf ref="ps2Data" type="Absolute16"/>

			<cpu:ReturnFromSubroutine/>
		</scope>

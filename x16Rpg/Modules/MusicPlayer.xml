		<!--01-->
		<byte>120</byte>
		<byte>60</byte>
		<byte>30</byte>
		<byte>15</byte>
		<byte>7</byte>
		<byte>90</byte>
		<byte>40</byte>
		<byte>20</byte>
		<byte>120</byte>
		<byte>60</byte>

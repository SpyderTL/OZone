	<label id="wait">
		<cpu:CallImmediate16Address/>
		<addressOf ref="Kernal.GetDateTime" type="Absolute16"/>

		<cpu:CopyImmediate8AddressToAccumulator/>
		<hex>08</hex>

		<cpu:CompareAccumulatorToImmediate16Address/>
		<addressOf ref="time" type="Absolute16"/>

		<cpu:BranchToRelative8IfEqual/>
		<addressOf ref="wait" type="Relative8"/>

		<cpu:CopyAccumulatorToImmediate16Address/>
		<addressOf ref="time" type="Absolute16"/>

		<cpu:ReturnFromSubroutine/>

		<var:byte id="time">0</var:byte>

		<label id="Kernal.GetDateTime" offset="FF50"/>
	</label>

	<label id="updateObjects">
		<cpu:CopyImmediate8ToAccumulator/>
		<byte>0</byte>

		<cpu:CopyAccumulatorToImmediate8Address/>
		<hex>22</hex>

		<label id="forEachObject"/>

		<cpu:CopyImmediate8AddressToXIndex/>
		<hex>22</hex>

		<cpu:CopyImmediate16PlusXIndexAddressToAccumulator/>
		<addressOf ref="Object.Type" type="Absolute16"/>

		<cpu:CompareAccumulatorToImmediate8/>
		<byte>0</byte>

		<cpu:BranchToRelative8IfEqual/>
		<addressOf ref="empty" type="Relative8"/>

		<cpu:CallImmediate16Address/>
		<addressOf ref="updateObject" type="Absolute16"/>

		<label id="empty"/>

		<cpu:IncrementImmediate8Address/>
		<hex>22</hex>

		<cpu:BranchToRelative8IfNotZero/>
		<addressOf ref="forEachObject" type="Relative8"/>

		<cpu:ReturnFromSubroutine/>
	</label>

	<label id="updateObject">
		<cpu:CopyImmediate8AddressToAccumulator/>
		<hex>22</hex>

		<cpu:CopyAccumulatorToImmediate16Address/>
		<addressOf ref="Sprite.Index" type="Absolute16"/>

		<cpu:CopyAccumulatorToXIndex/>

		<cpu:CopyImmediate16PlusXIndexAddressToAccumulator/>
		<addressOf ref="Object.X" type="Absolute16"/>

		<cpu:CopyAccumulatorToImmediate16Address/>
		<addressOf ref="Sprite.X" type="Absolute16"/>

		<cpu:CopyImmediate16PlusXIndexAddressToAccumulator/>
		<addressOf ref="Object.X2" type="Absolute16"/>

		<cpu:CopyAccumulatorToImmediate16Address/>
		<addressOf ref="Sprite.X" type="Absolute16" offset="1"/>

		<cpu:CopyImmediate16PlusXIndexAddressToAccumulator/>
		<addressOf ref="Object.Y" type="Absolute16"/>

		<cpu:CopyAccumulatorToImmediate16Address/>
		<addressOf ref="Sprite.Y" type="Absolute16"/>

		<cpu:CopyImmediate16PlusXIndexAddressToAccumulator/>
		<addressOf ref="Object.Y2" type="Absolute16"/>

		<cpu:CopyAccumulatorToImmediate16Address/>
		<addressOf ref="Sprite.Y" type="Absolute16" offset="1"/>

		<cpu:CallImmediate16Address/>
		<addressOf ref="Sprite.UpdatePosition" type="Absolute16"/>

		<cpu:ReturnFromSubroutine/>
	</label>

	<label id="handleCollisions">
		<cpu:CopyImmediate8ToYIndex/>
		<byte>1</byte>

		<label id="loopY"/>

		<cpu:CopyImmediate8ToXIndex/>
		<byte>0</byte>

		<label id="loopX"/>

		<cpu:CopyImmediate16PlusXIndexAddressToAccumulator/>
		<addressOf ref="Object.Type" type="Absolute16"/>

		<cpu:CompareAccumulatorToImmediate16PlusYIndexAddress/>
		<addressOf ref="Object.Type" type="Absolute16"/>

		<cpu:BranchToRelative8IfEqual/>
		<addressOf ref="noCollision" type="Relative8"/>

		<cpu:AndAccumulatorWithImmediate16PlusYIndexAddress/>
		<addressOf ref="Object.Type" type="Absolute16"/>

		<cpu:BranchToRelative8IfZero/>
		<addressOf ref="noCollision" type="Relative8"/>

		<cpu:CallImmediate16Address/>
		<addressOf ref="handleCollision" type="Absolute16"/>

		<label id="noCollision"/>

		<cpu:IncrementXIndex/>
		<cpu:IncrementXIndex/>

		<cpu:CompareXIndexToImmediate8/>
		<byte>128</byte>

		<cpu:BranchToRelative8IfNotEqual/>
		<addressOf ref="loopX" type="Relative8"/>

		<cpu:IncrementYIndex/>
		<cpu:IncrementYIndex/>

		<cpu:CompareYIndexToImmediate8/>
		<byte>129</byte>

		<cpu:BranchToRelative8IfNotEqual/>
		<addressOf ref="loopY" type="Relative8"/>

		<cpu:ReturnFromSubroutine/>
	</label>

	<label id="handleCollision">
		<scope>
			<cpu:SetCarryFlag/>

			<cpu:CopyImmediate16PlusXIndexAddressToAccumulator/>
			<addressOf ref="Object.Y" type="Absolute16"/>

			<cpu:SubtractImmediate16PlusYIndexAddressFromAccumulator/>
			<addressOf ref="Object.Y" type="Absolute16"/>

			<cpu:CopyAccumulatorToImmediate8Address/>
			<hex>02</hex>

			<cpu:CopyImmediate16PlusXIndexAddressToAccumulator/>
			<addressOf ref="Object.Y2" type="Absolute16"/>

			<cpu:SubtractImmediate16PlusYIndexAddressFromAccumulator/>
			<addressOf ref="Object.Y2" type="Absolute16"/>

			<cpu:CopyAccumulatorToImmediate8Address/>
			<hex>03</hex>

			<cpu:ClearCarryFlag/>

			<cpu:CopyImmediate8AddressToAccumulator/>
			<hex>02</hex>

			<cpu:AddImmediate8ToAccumulator/>
			<byte>8</byte>

			<cpu:CopyImmediate8AddressToAccumulator/>
			<hex>03</hex>

			<cpu:AddImmediate8ToAccumulator/>
			<byte>0</byte>

			<cpu:BranchToRelative8IfPositive/>
			<addressOf ref="notAbove" type="Relative8"/>

			<cpu:ReturnFromSubroutine/>

			<label id="notAbove"/>

			<cpu:SetCarryFlag/>

			<cpu:CopyImmediate8AddressToAccumulator/>
			<hex>02</hex>

			<cpu:SubtractImmediate8FromAccumulator/>
			<byte>8</byte>

			<cpu:CopyImmediate8AddressToAccumulator/>
			<hex>03</hex>

			<cpu:SubtractImmediate8FromAccumulator/>
			<byte>0</byte>

			<cpu:BranchToRelative8IfNegative/>
			<addressOf ref="notBelow" type="Relative8"/>

			<cpu:ReturnFromSubroutine/>

			<label id="notBelow"/>

			<cpu:SetCarryFlag/>

			<cpu:CopyImmediate16PlusXIndexAddressToAccumulator/>
			<addressOf ref="Object.X" type="Absolute16"/>

			<cpu:SubtractImmediate16PlusYIndexAddressFromAccumulator/>
			<addressOf ref="Object.X" type="Absolute16"/>

			<cpu:CopyAccumulatorToImmediate8Address/>
			<hex>02</hex>

			<cpu:CopyImmediate16PlusXIndexAddressToAccumulator/>
			<addressOf ref="Object.X2" type="Absolute16"/>

			<cpu:SubtractImmediate16PlusYIndexAddressFromAccumulator/>
			<addressOf ref="Object.X2" type="Absolute16"/>

			<cpu:CopyAccumulatorToImmediate8Address/>
			<hex>03</hex>

			<cpu:ClearCarryFlag/>

			<cpu:CopyImmediate8AddressToAccumulator/>
			<hex>02</hex>

			<cpu:AddImmediate8ToAccumulator/>
			<byte>8</byte>

			<cpu:CopyImmediate8AddressToAccumulator/>
			<hex>03</hex>

			<cpu:AddImmediate8ToAccumulator/>
			<byte>0</byte>

			<cpu:BranchToRelative8IfPositive/>
			<addressOf ref="notRight" type="Relative8"/>

			<cpu:ReturnFromSubroutine/>

			<label id="notRight"/>

			<cpu:SetCarryFlag/>

			<cpu:CopyImmediate8AddressToAccumulator/>
			<hex>02</hex>

			<cpu:SubtractImmediate8FromAccumulator/>
			<byte>8</byte>

			<cpu:CopyImmediate8AddressToAccumulator/>
			<hex>03</hex>

			<cpu:SubtractImmediate8FromAccumulator/>
			<byte>0</byte>

			<cpu:BranchToRelative8IfNegative/>
			<addressOf ref="notLeft" type="Relative8"/>

			<cpu:ReturnFromSubroutine/>

			<label id="notLeft"/>

			<cpu:PullAccumulatorFromStack/>
			<cpu:PullAccumulatorFromStack/>

			<cpu:PullAccumulatorFromStack/>
			<cpu:PullAccumulatorFromStack/>

			<cpu:ReturnFromSubroutine/>
		</scope>
	</label>


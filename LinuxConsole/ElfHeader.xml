<?xml version="1.0" encoding="utf-8" ?>

<scope	xmlns="http://metalx.org/Program">
	<label id="header"/>
	
	<!--Signature-->
	<hex>7f</hex>
	<string>ELF</string>
	
	<!--64-Bit-->
	<byte>2</byte>

	<!--Little Endian-->
	<byte>1</byte>

	<!--Version-->
	<byte>1</byte>

	<!--OS-->
	<byte>0</byte>

	<!--ABI-->
	<byte>0</byte>

	<!--Reserved-->
	<empty length="7"/>

	<!--Type: Executable-->
	<short>2</short>

	<!--Platform: x64-->
	<hex>3E</hex>
	
	<!--ELF Version-->
	<int>4</int>
	
	<!--Entry Point-->
	<addressOf ref="Application.EntryPoint" type="Absolute64"/>

	<!--Program Table-->
	<addressOf ref="programTable" type="Absolute64"/>
	
	<!--Section Table-->
	<long>0</long>
	
	<!--Flags-->
	<int>0</int>

	<!--Header Length-->
	<length from="header" to="header.end" type="Absolute16"/>
	
	<!--Program Length-->
	<short>56</short>
	
	<!--Program Count-->
	<short>1</short>
	
	<!--Section Length-->
	<short>0</short>
	
	<!--Section Count-->
	<short>0</short>
	
	<!--Section Name Index-->
	<short>0</short>
	
	<label id="header.end"/>

	<label id="programTable"/>

	<!--Type: Load-->
	<int>1</int>

	<!--Flags: Executable, Read, Write-->
	<int>7</int>

	<!--File Offset-->
	<length from="header" to="Application.EntryPoint" type="Absolute64"/>
	
	<!--Virtual Address-->
	<addressOf ref="Application.EntryPoint" type="Absolute64"/>
	
	<!--Reserved-->
	<long>0</long>
	
	<!--File Size-->
	<length from="Application.EntryPoint" to="Project.End" type="Absolute64"/>
	
	<!--Virtual Size-->
	<length from="Application.EntryPoint" to="Project.End" type="Absolute64"/>
	
	<!--Alignment-->
	<long>1</long>
</scope>